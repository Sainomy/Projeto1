<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Usuario - LIST</title>
    <style>
        table, td, th { 
            border: solid 1px black;
        }
        td, th{
            padding: 20px;
        }
        th{
            font-weight: bold;
            background-color: gray;
        }
    </style>
</head>
<body>
    <table>
        <tr>
            <th>id</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Senha</th>
            <th>Foto</th>
            <th>Delete</th>
        </tr>
        <% Usuarios.forEach(function(usuario){ %>
        <tr>
            <td> <%=usuario._id%> </td>
            <td> <%=usuario.nome%> </td>
            <td> <%=usuario.email%> </td>
            <td> <%=usuario.senha%> </td>
            <td> <%=usuario.foto%> </td>
            <td><a href="/usuario/del/<%=usuario._id%> ">Deletar</a></td>
        </tr>
        <% }) %>
       
    </table>
</body>
</html>